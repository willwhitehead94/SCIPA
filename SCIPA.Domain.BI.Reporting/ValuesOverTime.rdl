    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>SCIPADatabase</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@DeviceId">
            <Value>=Parameters!DeviceId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT Devices.Id, [Values].EventTime, [Values].FloatValue
FROM   [Values] INNER JOIN
          Devices ON [Values].DeviceId = Devices.Id
WHERE  ([Values].DeviceId = @DeviceId)</CommandText>
      </Query>
      <Fields>
        <Field Name="Id">
          <DataField>Id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="EventTime">
